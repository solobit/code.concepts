goog.provide "myproject.helpers"

myproject.helpers =
  ###*
  Adds two numbers and returns the result
  @param {number} a
  @param {number} b
  @returns {number} The sum of the numbers
  ###
  add: (a, b) -> a + b

  ###*
  Multiplies two numbers and returns the result
  @param {number} a
  @param {number} b
  @returns {number} The product of the numbers
  ###
  multiply: (a, b) -> a * b

  ###*
  Squares a number
  @param {number} a
  @returns {number} The square of the number
  ###
  square: (a) -> a * a
